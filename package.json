{
  "name": "startify",
  "displayName": "Flutter Boilerplate Generator",
  "description": "Generate boilerplate files and folders for new Flutter projects.",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeSadhu/boilerplate-vscode.git"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.generateBoilerplate"
  ],
  "publisher": "CodeSadhu",
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "startify.initiate",
        "title": "Startify: Generate Flutter Boilerplate"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "vsce-package": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "vsce": "^1.87.0"
  },
  "dependencies": {
    "npm": "^10.2.5"
  }
}
